    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.13",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "eslint": "^7.8.1",
    "eslint-config-react-native-wcandillon": "^3.3.3",
    "jest-expo": "^38.0.2",
    "typescript": "~3.9.5"
